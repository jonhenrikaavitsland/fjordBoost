---
import About from '../components/About.astro';
import Contact from '../components/Contact.astro';
import EmailCTA from '../components/EmailCTA.astro';
import Hero from '../components/Hero.astro';
import Proof from '../components/Proof.astro';
import Services from '../components/Services.astro';
import BaseLayout from '../layouts/BaseLayout.astro';

const title = 'Home';
const description =
  'Expert SEO for startups, small businesses and scale-ups. Affordable sprints, technical audits & web optimisation. Based in Oslo.';
---

<BaseLayout title={title} description={description}>
  <Hero />
  <Services />
  <About />
  <Proof />
  <EmailCTA />
  <Contact />
</BaseLayout>
